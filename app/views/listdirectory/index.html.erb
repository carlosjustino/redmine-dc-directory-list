<%= stylesheet_link_tag "jquery.dataTables.min.css", :plugin => 'listdirectory' %>
<%= javascript_include_tag "jquery-1.12.4.js", "jquery.dataTables.min.js", "jquery.blockUI.js", "anexosfontes.js", "tabela.js", :plugin => 'listdirectory' %>

<%= render "cabecalho" %>

<div id="telaview">
  <div id="opcoestela">
    <p>
      <%= form_tag(:action => "removerfontes") do -%>
        <%= hidden_field_tag 'issue_id', params[:issue_id] %>
        <%= hidden_field_tag 'arquivosRemover', "[]" %>
        <%= submit_tag "Remover Selecionados", :id => "arquivosRemoverButton" %>
      <% end -%>
    </p>
  </div>
  <%=
    @arquivostable = @arquivobanco
    render "tablefontes"
  %>
</div>

