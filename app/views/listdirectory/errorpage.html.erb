
<h1>Progress</h1>
<h2>Tarefa
  #<%= link_to @issue.id, issue_path(id: @issue.id), :onclick => params[:action] == 'show' ? "$('#update').hide(); return false;" : '' %>
  - <%= @clienteprogress %></h2>

<div id="telaerro">

  <% if @error %>
    <table>
      <thead>
        <tr>
          <th>Nome</th>
          <th>Texto</th>
        </tr>
      </thead>
    <tbody>
      <tr>
        <td>Classe: </td>
        <td><%= @classeTelaError %></td>
      </tr>
      <tr>
        <td>Mensagem:</td>
        <td><%= @mensagemTelaErro %></td>
      </tr>
      <tr>
        <td>Backtrace:</td>
        <td><%= @backtraceTelaErro %></td>
      </tr>
    </tbody>
    </table>
  <% end %>
</div>